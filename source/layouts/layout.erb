</head>
<body class="<%= page_classes %> <%= yield_content :page_classes %>">

  <header class="site-header">
    <%= partial 'partials/header-toolbar' %>
    <%= yield_content :site_header %>
  </header>
  <!-- /site-header -->
  
  <main class="main-content">
    <%= yield %>
  </main><!-- /main-content -->

  <footer class="site-footer">
    <%= partial 'partials/footer-content' %>
  </footer><!-- /site-footer -->
  

  <%= javascript_include_tag "public" %>
